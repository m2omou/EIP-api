<h1>Listing publications</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>user_id</th>
      <th>category_id</th>
      <th>media_id</th>
      <th>place_id</th>
      <th>title</th>
      <th>content</th>
      <th>show</th>
      <th>edit</th>
      <th>delete</th>
    </tr>
  </thead>

  <tbody>
    <% @publications.each do |publication| %>
      <tr>
      	<td><%= publication.id %></td>
      	<td><%= publication.user_id %></td>
      	<td><%= publication.category_id %></td>
      	<td><%= publication.media_id %></td>
      	<td><%= publication.place_id %></td>
      	<td><%= publication.title %></td>
      	<td><%= publication.content %></td>
        <td><%= link_to 'Show', publication %></td>
        <td><%= link_to 'Edit', edit_publication_path(publication) %></td>
        <td><%= link_to 'Destroy', publication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publication', new_publication_path %>
